[
  {
    "name": "Country",
    "table": "countries",
    "attributes": [
      {"name": "id", "type": "uuid", "primary_key": true, "nullable": false},
      {"name": "code", "type": "string", "nullable": false, "length": 3},
      {"name": "name", "type": "string", "nullable": false}
    ]
  },
  {
    "name": "Equipment",
    "table": "equipment",
    "attributes": [
      {"name": "id", "type": "uuid", "primary_key": true, "nullable": false},
      {"name": "serial_number", "type": "string", "nullable": false},
      {"name": "type", "type": "string", "nullable": false},
      {"name": "country_id", "type": "reference", "target": "countries.id", "nullable": false},
      {"name": "owner_id", "type": "reference", "target": "employees.id", "nullable": true},
